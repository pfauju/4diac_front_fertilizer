	IN_01 : BYTE; // BYTE 01
	IN_02 : BYTE; // BYTE 02
	IN_03 : BYTE; // BYTE 03
	IN_04 : BYTE; // BYTE 04
	IN_05 : BYTE; // BYTE 05
	IN_06 : BYTE; // BYTE 06
	IN_07 : BYTE; // BYTE 07
END_VAR

VAR_OUTPUT
