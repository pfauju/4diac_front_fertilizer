END_VAR

OUT[0] := IN_00;
OUT[1] := IN_01;
OUT[2] := IN_02;
OUT[3] := IN_03;
OUT[4] := IN_04;
OUT[5] := IN_05;
OUT[6] := IN_06;
OUT[7] := IN_07;

