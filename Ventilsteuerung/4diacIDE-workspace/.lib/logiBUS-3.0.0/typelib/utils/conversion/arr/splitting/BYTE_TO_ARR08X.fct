	<FunctionBody>
		<ST><![CDATA[PACKAGE logiBUS::utils::conversion::arr::splitting;

(* this Function extracts the 8 BOOLS from a byte and make a 8-BOOL Array *)
FUNCTION BYTE_TO_ARR08X

VAR_INPUT
	IN : BYTE; // BYTE Input
END_VAR

VAR_OUTPUT
	OUT : ARRAY[0..7] OF BOOL; // ARR08B Output
END_VAR

OUT[0] := IN.0;
OUT[1] := IN.1;
OUT[2] := IN.2;
OUT[3] := IN.3;
OUT[4] := IN.4;
OUT[5] := IN.5;
OUT[6] := IN.6;
OUT[7] := IN.7;

END_FUNCTION
]]></ST>
